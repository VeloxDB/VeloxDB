{
  "metadata": [
    {
      "src": [
				{
					"src":"../../bin",
					"files":["vlxdb.dll", "vlxc.dll", "vlxasp.dll"]
				},
				{
					"src":"../bin",
					"files":["vlxdb.dll", "vlxc.dll", "vlxasp.dll"]
				}
			],
      "dest": "api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
			"properties":{
				"TargetFramework":"net7.0"
			},
			"filter":"filter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "guide/**.md",
          "guide/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
					"logo.svg",
					"favicon.ico",
					"robots.txt",
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../bin/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
			"default",
			"templates/custom"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}